package carcompany;

import java.util.ArrayList;
import java.util.List;

public class Factory {
	List<Car> carList = new ArrayList<Car>();

	public Car make(String name, int number) {

		// Car cars[];
		for (int i = 0; i < number; i++) {
			if (name == "sonata") {
				Sonata s = new Sonata();
				carList.add(s);
			}

			if (name == "genesis") {

				Genesis g = new Genesis();
				carList.add(g);
			}
			if (name == "grandeur") {
				Grandeur gr = new Grandeur();
				carList.add(gr);

			}
		}

		Genesis g = new Genesis();

		Grandeur gr = new Grandeur();

		return null;
	}
}
