package carcompany;

public class Factory {

	public Car make(String name, int number) {
		Car cars[] = null;
		int i =0 ;
		
		if (name == "sonata") {
			for (; i < number; i++) {
				Sonata s = new Sonata();
				cars[i] = s;
			}
		}
		if(name == "genesis"){
			for(; i < number; i++){
				Genesis g = new Genesis();
				cars[i] = g;
			}
		}
		if(name == "grandeur"){
			for(; i <number ; i++){
				Grandeur gr = new Grandeur();
				cars[i] = gr;
			}
		}
		
		Genesis g = new Genesis();

		Grandeur gr = new Grandeur();

		return null;
	}
}
